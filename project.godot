; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="HanaBeat"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DATA="*res://Autoloads/Data.gd"
EVENTS="*res://Autoloads/Events.gd"
GAME="*res://Autoloads/Game.gd"
LOCALIZATION="*res://Autoloads/Localization.gd"

[display]

window/size/viewport_width=600
window/size/viewport_height=1080
window/handheld/orientation=1

[input_devices]

pointing/emulate_touch_from_mouse=true

[internationalization]

locale/translations=PackedStringArray("res://Localization/translations.en.translation", "res://Localization/translations.fr.translation")

[rendering]

renderer/rendering_method="mobile"
